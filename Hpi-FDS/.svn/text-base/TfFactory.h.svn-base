#import <Foundation/Foundation.h>
#import <sqlite3.h>
@interface TfFactory : NSObject{
    NSString    *FACTORYCODE;
    NSString    *FACTORYNAME;
    NSString    *CAPACITYSUM;
    NSString    *DESCRIPTION;
    NSInteger   SORT;
    NSInteger   BERTHNUM;
    NSString    *BERTHWET;
    NSString    *CHANNELDEPTH;
    NSString    *CATEGORY;
    NSInteger   MAXSTORAGE;
    NSString    *ORGANCODE;
}
@property (nonatomic,retain) NSString *FACTORYCODE;
@property (nonatomic,retain) NSString *FACTORYNAME;
@property (nonatomic,retain) NSString *CAPACITYSUM;
@property (nonatomic,retain) NSString *DESCRIPTION;
@property (nonatomic,retain) NSString *BERTHWET;
@property (nonatomic,retain) NSString *CHANNELDEPTH;
@property (nonatomic,retain) NSString *CATEGORY;
@property (nonatomic,retain) NSString *ORGANCODE;

@property NSInteger SORT;
@property NSInteger BERTHNUM;
@property NSInteger MAXSTORAGE;

@end
