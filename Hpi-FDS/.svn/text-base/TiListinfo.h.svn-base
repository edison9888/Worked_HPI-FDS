//INFOID               INFOID
//COLUMNS              列号
//ROWS                 行号
//TITLE                标题
//DATAVALUE            数据值
//DECLENGTH            小数位数
//URL                  链接地址


#import <Foundation/Foundation.h>
#import <sqlite3.h>
@interface TiListinfo : NSObject{
    NSInteger infoId;
    NSInteger columns;
    NSInteger rows;
    NSString  *title;
    NSString  *dataValue;
    NSInteger decLength;
    NSString  *url;
}

@property NSInteger infoId;
@property NSInteger columns;
@property NSInteger rows;
@property (nonatomic, retain) NSString *title;
@property (nonatomic, retain) NSString *dataValue;
@property NSInteger decLength;
@property (nonatomic, retain) NSString *url;

@end
